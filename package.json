  "description": "Build a great looking personal resume in seconds with JSON resumes, React and the Bulma css framework.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
